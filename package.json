{
	"name": "@elsikora/x-captcha-client",
	"version": "1.0.0",
	"description": "Client library for X-Captcha service",
	"keywords": [
		"captcha",
		"security",
		"client"
	],
	"license": "MIT",
	"author": "",
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js"
		}
	},
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "rollup -c",
		"clean": "rimraf dist",
		"commit": "cz",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"lint": "eslint ./",
		"lint:all": "npm run lint && npm run lint:types",
		"lint:all:fix": "npm run lint:fix && npm run lint:types:fix",
		"lint:fix": "eslint --fix ./",
		"lint:types": "tsc --noEmit",
		"lint:types:fix": "tsc --noEmit --skipLibCheck",
		"prepare": "husky",
		"release": "semantic-release",
		"test": "jest",
		"typecheck": "tsc --noEmit"
	},
	"config": {
		"commitizen": {
			"path": "@elsikora/commitizen-plugin-commitlint-ai"
		}
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.0",
		"@commitlint/config-conventional": "^19.8.0",
		"@elsikora/commitizen-plugin-commitlint-ai": "^1.0.0",
		"@elsikora/eslint-config": "^3.7.9",
		"@rollup/plugin-typescript": "^12.1.2",
		"commitizen": "^4.3.1",
		"conventional-changelog-conventionalcommits": "^8.0.0",
		"eslint": "^9.22.0",
		"eslint-plugin-n": "^17.16.2",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.0",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"rollup": "^4.36.0",
		"rollup-plugin-generate-package-json": "^3.2.0",
		"typescript": "^5.8.2"
	}
}
